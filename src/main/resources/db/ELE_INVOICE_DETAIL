CREATE TABLE ELE_INVOICE_DETAIL (
ELE_INVOICE_DETAIL_ID int(20) NOT NULL AUTO_INCREMENT COMMENT '电子发票明细表ID/PK',   --ELE_INVOICE_LN_ID
ELE_INVOICE_ID int(20) NOT NULL COMMENT '电子发票ID',  --ELE_INVOICE_HD_ID
INVOICE_ID int(20) NOT NULL COMMENT '发票表ID',   --INVOICE_HD_ID
INVOICE_DETAIL_ID int(20) NOT NULL COMMENT '发票明细表ID',   --INVOICE_LN_ID
CONTRACT_NO varchar(30) NULL COMMENT '合同号',   --CONTRACT_ID
BILL_NO varchar(30) NULL COMMENT '订单号',
BILL_NAME varchar(30) NULL COMMENT '商品名称',   --NAME
BILL_CODE varchar(50) NULL COMMENT '商品编号（税收分类编码）',  --CODE
LINE_TYPE varchar(3) NULL COMMENT '发票行性质 0代表正常行',
SPEC varchar(30) NULL COMMENT '规格型号',
UNIT varchar(5) NULL COMMENT '计量单位',
TAX_RATE decimal(4) NULL COMMENT '税率',
TAX_QUANTITY int(4) NULL COMMENT '数量',  --quantity
TAX_PRICE decimal(12) NULL COMMENT '单价',
TOTAL_AMOUNT decimal(12) NULL COMMENT '含税金额',
YHZCBS varchar(12) NULL COMMENT '税收优惠政策标志',
YHZCNR varchar(120) NULL COMMENT '享受税收优惠政策内容',
LSLBS int(4) NULL COMMENT '零税率标识 空表示非0税率',
ZXBM varchar(120) NULL COMMENT '自行编码',
KCE decimal(12) NULL COMMENT '扣除额',
CREATED_BY int(10) NULL COMMENT '创建人',
CREATE_TIME timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
UPDATED_BY int(10) NULL COMMENT '更新人',
UPDATE_TIME timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
PRIMARY KEY (`ELE_INVOICE_DETAIL_ID`)
)ENGINE=InnoDB AUTO_INCREMENT=10783 DEFAULT CHARSET=utf8 COMMENT='电子发票明细表';





