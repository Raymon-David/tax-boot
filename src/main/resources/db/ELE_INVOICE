CREATE TABLE ELE_INVOICE (
ELE_INVOICE_ID int(20) NOT NULL AUTO_INCREMENT COMMENT '电子发票ID/PK',
INVOICE_ID int(20) NOT NULL COMMENT '发票表ID',
DOCUMENT_TYPE varchar(30) NULL COMMENT '单据类型',
DOCUMENT_CATEGORY varchar(30) NULL COMMENT '单据类别',
SERIAL_NUM varchar(20) NOT NULL COMMENT '流水号', --SWNO
SALE_TAX varchar(50) DEFAULT '' COMMENT '销方税号',
CUST_NAME varchar(100) DEFAULT '' COMMENT '购方名称',
CUST_TAX_NO varchar(100) NULL COMMENT '购方税号',
CUST_ADDR varchar(200) NULL COMMENT '购方地址',
CUST_TELEPHONE varchar(100) NOT NULL COMMENT '购货方手机号',
CUST_PHONE varchar(100) NULL COMMENT '购方固定电话',
CUST_EMAIL varchar(200) NULL COMMENT '购方邮箱',
CUST_BANK_ACCOUNT varchar(200) NULL COMMENT '开户行+账号',
CUST_TYPE varchar(200) NULL COMMENT '购货方企业类型',
ELE_INVOICE_MEMO varchar(200) NULL COMMENT '备注',--INVO_MEMO
ELE_INVOICE_TYPE varchar(5) NOT NULL COMMENT '发票类型',--INV_TYPE
BILL_DATE timestamp NULL COMMENT '开票日期',
CANCEL_NUM varchar(20) NULL COMMENT '退货单号',--THDH
BILL_TYPE varchar(5) NOT NULL COMMENT '开票类型',
SPECIAL_RED_FLAG varchar(5) NOT NULL COMMENT '特殊红冲标志',
OPERATION_CODE varchar(5) NOT NULL COMMENT '操作代码',
ISSUER varchar(20) NOT NULL COMMENT '开票人',  --KPY
PAYEE varchar(20) NULL COMMENT '收款员', --SKY
REVIEWER varchar(20) NULL COMMENT '复核人',--FHR
FORMER_INVOICE_CODE varchar(20) NULL COMMENT '原发票代码',--YFPDM
FORMER_INVOICE_NUM varchar(20) NULL COMMENT '原发票号码',--YFPHM
RED_REASON varchar(200) NULL COMMENT '红冲原因', --CHYY
ELE_INVOICE_STATUS varchar(10) NULL COMMENT '电子发票状态',--STATUS
PDF_URL varchar(2000) NULL COMMENT '电子接口返回的pdf附件URL',
ERROR_CODE varchar(20) NULL COMMENT '开票日期',
ERROR_MESSAGE varchar(200) NULL COMMENT '电子发票返回错误message',
CREATED_BY int(10) NULL COMMENT '创建人',
CREATE_TIME timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
UPDATED_BY int(10) NULL COMMENT '更新人',
UPDATE_TIME timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
PRIMARY KEY (`ELE_INVOICE_ID`)
)ENGINE=InnoDB AUTO_INCREMENT=10783 DEFAULT CHARSET=utf8 COMMENT='电子发票表';



