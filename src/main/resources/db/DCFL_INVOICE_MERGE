CREATE VIEW DCFL_INVOICE_MERGE AS
SELECT
		BILL_TYPE ,
		INVOICE_CODE ,
		INVOICE_NUMBER ,
		INVOICE_TITLE ,
		TAX_REGISTRY_NUM ,
		INVOICE_OBJECT_ADDRESS_PHONE ,
		INVOICE_OBJECT_BANK_ACCOUNT ,
		ISSUED_TIME ,
		ISSUED_MONTH ,
		TOTAL_AMOUNT ,
		TAX_NET_AMOUNT ,
		TAX_RATE ,
		TAX_AMOUNT ,
		PRODUCT_NAME ,
		INVOICE_MEMO ,
		ISSUER ,
		ACCOUNT_PAYEE ,
		REVIEWER ,
		INVOICE_INVALID_FLAG
FROM DCFL_ELE_INVOICE_IMPORT_TEMP
UNION ALL
SELECT
		BILL_TYPE ,
		INVOICE_CODE ,
		INVOICE_NUMBER ,
		INVOICE_TITLE ,
		TAX_REGISTRY_NUM ,
		INVOICE_OBJECT_ADDRESS_PHONE ,
		INVOICE_OBJECT_BANK_ACCOUNT ,
		ISSUED_TIME ,
		ISSUED_MONTH ,
		TOTAL_AMOUNT ,
		TOTAL_AMOUNT - TAX_AMOUNT AS TAX_NET_AMOUNT ,
		TAX_RATE ,
		TAX_AMOUNT ,
		PRODUCT_NAME ,
		INVOICE_MEMO ,
		ISSUER ,
		ACCOUNT_PAYEE ,
		REVIEWER ,
		INVOICE_INVALID_FLAG
FROM DCFL_PAPER_INVOICE_IMPORT_TEMP